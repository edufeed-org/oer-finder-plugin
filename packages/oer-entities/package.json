{
  "name": "@edufeed-org/oer-entities",
  "version": "0.0.1",
  "description": "TypeORM entities for OER aggregation",
  "author": "B310 Digital GmbH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/edufeed-org/oer-finder-plugin.git",
    "directory": "packages/oer-entities"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "peerDependencies": {
    "typeorm": "^0.3.0"
  },
  "peerDependenciesMeta": {
    "typeorm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "typeorm": "^0.3.27",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
