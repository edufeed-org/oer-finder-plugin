{
  "name": "@edufeed-org/oer-finder-api-client",
  "version": "0.0.9",
  "description": "Auto-generated API client for OER Proxy",
  "author": "B310 Digital GmbH",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/edufeed-org/oer-finder-plugin.git",
    "directory": "packages/oer-finder-api-client"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:spec": "cd ../.. && pnpm run generate:openapi",
    "generate:types": "pnpm exec openapi-typescript openapi.json -o ./generated/schema.ts",
    "generate": "pnpm run generate:spec && pnpm run generate:types",
    "build": "pnpm run generate:types && vite build && tsc --emitDeclarationOnly",
    "build:full": "pnpm run generate && vite build && tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.3"
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.3",
    "rollup-plugin-license": "^3.6.0",
    "typescript": "^5.7.3",
    "vite": "^6.4.1"
  }
}
